{% if menu %}
    <ul class="navbar-nav ml-auto">
        {% for item in menu %}
            <li class="{{ item.classes | join(' ') }} nav-item ">
                <a class="nav-link" target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
                {% include "menu.twig" with {'menu': item.get_children} %}
            </li>
        {% endfor %}
    </ul>
{% endif %}